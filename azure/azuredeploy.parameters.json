{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "southcentralus"
        },
        "networkInterfaceName": {
            "value": "infrastructure-virtu123" 
        },
        "enableAcceleratedNetworking": {
            "value": true 
        },
        "networkSecurityGroupName": {
            "value": "NSG-Infrastructure-Virtualization-Host-001"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name":"SSH",
                    "properties":{
                        "priority":300,
                        "protocol":"TCP",
                        "access":"Allow",
                        "direction":"Inbound",
                        "sourceAddressPrefix":"*",
                        "sourcePortRange":"*",
                        "destinationAddressPrefix":"*",
                        "destinationPortRange":"22"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkName": {
            "value": "NaaS-USAF-vnet"
        },
        "addressPrefixes": {
            "value": [
                "10.0.0.0/16"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name":"default",
                    "properties":{
                        "addressPrefix":"10.0.0.0/24"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "Infrastructure-Virtualization-Host-001-ip"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "virtualMachineName": {
            "value": "Infrastructure-Virtualization-Host-001"
        },
        "virtualMachineComputerName": {
            "value": "Infrastructure-Virtualization-Host-001"
        },
        "virtualMachineRG": {
            "value": "NaaS-USAF-Dev"
        },
        "osDiskType": {
            "value": "Premium_LRS"
        },
        "dataDisks": {
            "value": [
                {
                    "lun":0,
                    "createOption":"attach",
                    "caching":"None",
                    "writeAcceleratorEnabled":false,
                    "id":null,
                    "name":"Infrastructure-Virtualization-Host-001_DataDisk_0",
                    "storageAccountType":null,
                    "diskSizeGB":null,
                    "diskEncryptionSet":null
                }
            ]
        },
        "dataDiskResources": {
            "value": [
                {
                    "name":"Infrastructure-Virtualization-Host-001_DataDisk_0",
                    "sku":"Premium_LRS",
                    "properties":{
                        "diskSizeGB":1024,
                        "creationData":{
                            "createOption":"empty"
                        }
                    }
                }
            ]
        },
        "virtualMachineSize": {
            "value": "Standard_D16ds_v4"
        },
        "adminUsername": {
            "value": "nchao"
        },
        "adminPublicKey": {
            "value": "ssh-rsa "
        }
    }
}